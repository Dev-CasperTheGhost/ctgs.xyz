generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider          = "mysql"
    url               = env("DATABASE_URL")
    shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
    planetScaleMode   = true
}

model Url {
    id     String @id @default(cuid())
    slug   String @unique
    url    String
    clicks Int    @default(0)
}
